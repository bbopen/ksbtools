ifelse(MKCMD,`no',`syscmd(`make curly.c curly.h uncurly.c uncurly.h >/dev/null')')dnl
`( . ) -> ( 'HOST` )
	except_pat ( /RCS /Makefile /Distfile /Make\\.host );
	install ${INTO} ;

( @Make.host@ ) -> ( 'HOST` )
	install -b ${INTO}/Makefile ;
'dnl
