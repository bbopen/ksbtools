# $Id: markers,v 5.2 2007/02/09 02:28:50 ksb Exp $
$Compile: MK=-t/dev/null %b -a%C%N%I%V -mMarks -dSubs %f

# This file checks to be sure mk will find the correct marked line.

$Marks(*): [ "%M" = "marks" -a "%m" = "Marks" ]
$Marks: false
$Marks(notme): false
$Marks(*): [ "%S" = "subs" -a "%s" = "Subs" ]
$Marks(Subs): [ "%*" = "Marks(Subs)" ]
$Marks(*): [ "%*" = "Marks(*)" ]
$Marks(*): %hNew(waS)
$New(*): [ "%M" = "new" -a "%m" = "New" ]
$New: false
$New(notagain): false
$New(*): [ "%S" = "was" -a "%s" = "waS" ]
$New(*): Was%H
$New(Subs): false
$New(*): [ "%S" = "was" -a "%s" = "Was" ]
$New(*): [ "%*" = "New(*)" ]
