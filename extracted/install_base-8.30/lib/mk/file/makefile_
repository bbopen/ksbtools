$Compile(*):	Make%h

$Make(*):	${make-make} ${cc_debug+DEBUG=$cc_debug} ${cc+CC=$cc} -f %f %s
$Make(*):	${make-make} ${cc_debug+DEBUG=$cc_debug} ${cc+CC=$cc} -f %f
$Make(*):	${make-make} -f %f

$Clean(*):	${make-make} -f %f %s clean
$Clean(*):	${make-make} -f %f clean

$Depend(*):	${make-make} -f %f %s depend
$Depend(*):	${make-make} -f %f depend

$Info(*):	${echo-echo} "a make description file"
