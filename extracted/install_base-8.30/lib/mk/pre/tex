#  $Header: /usr/msrc/usr/local/lib/mk/pre/RCS/tex,v 5.0 1996/09/09 20:16:55 kb207252 Exp $
#
#  Determine whether a file is a TeX or a LaTeX file		(nuspl)
# $Id: tex,v 5.0 1996/09/09 20:16:55 kb207252 Exp $
#

$*(*):          if %~/bin/is-latex %f\n\tthen\n\t\t%b %o -m%m -d%s -e'%~/is-latex%%;' %f\n\telse\n\t\t%b %o -m%m -d%s -e'%~/is-tex%%;' %f\n\tfi

$*(*):          if %~/bin/is-latex %f\n\tthen\n\t\t%b %o -m%m -e'%~/is-latex%%;' %f\n\telse\n\t\t%b %o -m%m -e'%~/is-tex%%;' %f\n\tfi

