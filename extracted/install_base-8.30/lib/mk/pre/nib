# XXX magic number check?

$Compile(*):	${make-make} DEBUG=${cc_debug--O} %s
$Compile(*):	${make-make} DEBUG=${cc_debug--O}

$Clean(*):	${make-make} clean

$Display(*):	${open-open} %s %f
$Display(*):	${open-open} %f

$Info(*):	${echo-echo} "NeXT interface builder file"
